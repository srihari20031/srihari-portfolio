{
    "builds": [
      { "src": "package.json", "use": "@vercel/node" }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "api/*.js": {
        "memory": 1024,  
        "maxDuration": 10
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" }
    ]
  }